    <!-- Header -->
    <header class="masthead">
      <div class="container">
        <div class="intro-text">
          <div class="intro-lead-in">Welcome To The Barber Scheduler!</div>
          <div class="intro-heading text-uppercase">It's Nice To Meet You</div>

          {{#unless isLoggedIn}}
            <a class="btn btn-danger btn-xl text-uppercase" data-toggle="modal" href="#signInModal" id="sign-in">Sign In</a>
            <a class="btn btn-primary btn-xl text-uppercase" data-toggle="modal" href="#signUpModal" id="sign-up">Sign Up</a>
          {{/unless}}

        </div>
      </div>
    </header>

    <!-- Services -->
    <section class="bg-light" id="services">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase">Services</h2>
            <h3 class="section-subheading text-muted">We do the clerical stuff so you can focus on what you do best.</h3>
          </div>
        </div>
        <div class="row text-center">
          <div class="col-md-4">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x text-primary"></i>
              <i class="fa fa-calendar fa-stack-1x fa-inverse"></i>
            </span>
            <h4 class="service-heading">Appointments</h4>
            <p class="text-muted"> View all appointments on a customizable calander.</p>
          </div>
          <div class="col-md-4">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x text-primary"></i>
              <i class="fa fa-credit-card-alt fa-stack-1x fa-inverse"></i>
            </span>
            <h4 class="service-heading">Payments</h4>
            <p class="text-muted">Securely accept card and cryptocurrency payments without the fear of price swings by having it converted to cash right away. *Subject to applicable fees.</p>
          </div>
          <div class="col-md-4">
            <span class="fa-stack fa-4x">
              <i class="fa fa-circle fa-stack-2x text-primary"></i>
              <i class="fa fa-line-chart fa-stack-1x fa-inverse"></i>
            </span>
            <h4 class="service-heading">Dashboard</h4>
            <p class="text-muted">View historical client visit data from one central location to identify trends and offer promotions.</p>
          </div>
        </div>
      </div>
    </section>

